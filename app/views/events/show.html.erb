<div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url(https://images.pexels.com/photos/1763075/pexels-photo-1763075.jpeg?auto=compress&cs=tinysrgb&dpr=2&h=750&w=1260)">
 <h1> <%=current_user.events.name %></h1>
</div>

<div class="cards">
<div class="top-songs">
  <h1>Top 5 songs</h1>
  <% @tracks.each do |track| %>
    <% track.artists.each do |artist| %>
    <%= render "shared/artist_card", artist: artist %>
    <% end %>
  <% end %>
</div>

<div class="top-artists">
  <h1>Top 5 artists</h1>
  <% @event_users.each do |user| %>
    <% user.tracks_users.each do |tracks_user| %>
    <% artistid = ArtistsTrack.find(tracks_user.track_id).artist_id %>
    <%= artist = Artist.find(artistid) %>
    <%= render "shared/artist_card", artist: artist %>
    <% end %>
  <% end %>
</div>
</div>
